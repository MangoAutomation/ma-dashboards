<div class="container">
<div class="row clearfix">
    <div class="col-md-12 column">
        <nav class="navbar navbar-default" role="navigation">
        <div class="navbar-header">
        <select class="navbar-brand">
            <option value="" disabled selected>Meters</option>
            <option value="simpleTimePeriodType"></option>
        </select>
		</div>
</div>
</div>

    <div class="col-md-4  container-lg-height" style=" height:1000px">

<table height=100%>
    <!-- Lables for this row -->
    <tr>
        <td class="leftName">Amps</td>
        <td><div id="ampsBarChartDiv" ></div></td>
        <td><div id="ampsBarTextDiv" ></div></td> 
    </tr>
    <tr>    
        <td class="leftName">Volts</td> 
        <td><div id="voltsBarChartDiv"> </div></td>
        <td><div id="voltsTextDiv"> </div></td> 
    </tr>
    <!--the realtime numbers-->
    <tr>
        
        <td class="leftName">Kilo Watts</td>
        <td><div id="kwBarChartDiv" ></div></td> 
        <td><div id="kwBarTextDiv" ></div></td>         
    </tr>
    <!--the realtime numbers-->
    <tr>
        <td class="leftName">Power Factor</td>
        <td><div id="powerFactorBarChartDiv" ></div></td> 
        <td><div id="powerFactorTextDiv"></div></td>
        </tr>
</table>
    </div>
    
    
    
    
    
<!--    start top right -->
    
    
    <div class="col-md-8">
        <div class="changStatistics" style="width:30%; margin-right:auto; margin-left: 60% ">
            <button class="btn" onClicked="#main">History</button>
            <button class="btn" href="#statisticsPage">Statistics</button>
        </div>
    <div id="kwhBarChartDiv" style="width:100%; height:400px; vertical-align: top;  display: inline-block; padding: 10px;">
    </div>
  <h3>KWH Watts Per Day</h3>
<!-- Ui Wiget for hourly time Picker-->
<div class="row">
    <div class="col-md-2">
        <select id="simpleTimePicker" class="form-control">
            <option value="" disabled selected>Today</option>
            <option value="simpleTimePeriodType"></option>
        </select>
    </div>
    
    <div class="col-md-2">
        <input class="form-control" placeholder="From Date" id="startDate"> </input>
    </div>
    
    <div class="col-md-2">
        <input class="form-control" placeholder="To Date" id="endDate"></input>
    </div>
    
    <div class="col-md-2">
        <select id="simpleTimePeriodType" class="form-control">
            <option value="" disabled selected>Hourly</option>
            <option value="simpleTimePeriodType"></option>
        </select>
    </div>
</div>
</div>
    
    
    
    
    

<!--The tabs for the charts  -->
<div id="tab"  class="col-md-8">
    <div>
        <ul class="nav nav-tabs" role="tablist">
            <li onclick="templater.invalidateChartSize(['kwhChartDiv'], templater);">
            <button class="btn" href="#kw">KW</button>
            </li>
            
            <li onclick="templater.invalidateChartSize(['ampsChartDiv'], templater);">
            <button class="btn" href="#amps">Amps</button>
            </li>
            
            <li onclick="templater.invalidateChartSize(['voltsLineChartDiv'], templater);">
            <button class="btn" onclick=""href="#volts">Volts</button>
            </li>
        </ul>
    </div>

    <div id="kw">
        <div id="kwhChartDiv" >
        </div>
    </div>
    <div id="amps">
        <div id="ampsChartDiv" >
        </div>
    </div>
    
    <div id="volts">
        <div id="voltsLineChartDiv">
        </div>
    </div>



    <!--Starting the Rollup and date and time pickers -->
    <div class="row">
        
            <div class="col-md-2">
                <input placeholder="Start Date" id="startDate" class="form-control"></input>
            </div>
         
            <div class="col-md-2">
                <input placeholder="End Date"id="endDate" class="form-control"></input>
            </div>
            
            <div class="col-md-2">
                <select id="rollup" class="form-control">
                
                </select>
            </div>
       
       
            <div class="col-md-2">
                <select id="timePeriodType" class="form-control">
                
                </select>
            </div>
            
            <div class="col-md-2">
                <input placeholder="1" id="timePeriod" type="number" value="1" class="form-control"></input>
            </div>
        
        
    </div>
