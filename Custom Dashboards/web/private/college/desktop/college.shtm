        $('.chart-tabs').tabdrop();
        $('.overview-tabs').tabdrop();

        templater.invalidateChartSize(['kwhLineChartDiv'], templater);
        
        //Refresh the am chart for daily kWh
        for(var i=0; i<kWhDailyBarChartDisplayManager.displays.length; i++){
            var display = kWhDailyBarChartDisplayManager.displays[i];
            if($.inArray(display.divId, ['kWhDailyBarChartDiv']) >= 0){
               //Invlidate the chart size
               display.amChart.invalidateSize();
           }
        }
        
        
    });

});

